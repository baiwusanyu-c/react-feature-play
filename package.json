{
  "name": "react-feature-play",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/baiwusanyu-c/react-feature-play",
  "scripts": {
    "init": "pnpm i",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:ts": "eslint --fix ./ --ext .vue,.js,.ts,.jsx,.tsx",
    "prepare": "husky install",
    "husky:step1": "npx husky add .husky/pre-commit \"npm exec lint-staged --allow-empty\"",
    "husky:step2": "git add .husky/pre-commit && npx husky add .husky/commit-msg \"npx --no -- commitlint --edit $1\" && git add .husky/commit-msg",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "optimizeImages": "tiny --once"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "^5.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-router-dom": "^6.4.2",
    "react-syntax-highlighter": "^15.5.0",
    "remark-gfm": "^3.0.1"
  },
  "devDependencies": {
    "@baiwusanyu/eslint-config": "^1.0.6",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@docsearch/css": "^3.2.1",
    "@docsearch/js": "^3.2.1",
    "@types/node": "^18.7.14",
    "@types/prop-types": "^15.7.5",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@vitejs/plugin-react": "^2.1.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "easy-tinypng-cli": "^0.4.0",
    "eslint": "8.22.0",
    "husky": "^7.0.4",
    "prop-types": "^15.8.1",
    "sass": "^1.49.0",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vite-plugin-chunk-split": "^0.4.6",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mkcert": "^1.9.0",
    "vite-plugin-progress": "^0.0.6",
    "vite-plugin-windicss": "^1.8.8",
    "windicss": "^3.5.6"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint ./ --fix",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "@baiwusanyu"
    ]
  }
}
